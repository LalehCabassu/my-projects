<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.linedetection_houghtransforms"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.linedetection_houghtransforms.AcrivitySelector"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.linedetection_houghtransforms.EdgeDetectorAct"
            android:label="@string/title_activity_edge_detector" >
            <intent-filter>
                <action android:name="android.intent.action.EdgeDetectorAct" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.linedetection_houghtransforms.ImageDisplayer"
            android:label="@string/title_activity_image_displayer" >
            <intent-filter>
                <action android:name="android.intent.action.ImageDisplayer" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.linedetection_houghtransforms.HoughTransformsAct"
            android:label="@string/title_activity_hough_transforms" >
            <intent-filter>
                <action android:name="android.intent.action.HoughTransformsAct" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            
        </activity>
    </application>

</manifest>